@model IEnumerable<EProject.Models.Competition>

<div class="container mt-5">

    <div class="col-md-12 col-lg-12">
        <div class="card">
            <div class="card-header"> <span style="color: #F7631B;"> <h5> Competition Table </h5></span></></div>
            <div class="card-body">
                <p class="card-title"></p>
                <table class="table table-hover" id="dataTables-example" width="100%">
                    <thead>
                        <tr>
                            <th>Competition Title</th>
                            <th>Competition Banner</th>
                            <th>Competition Start-Date</th>
                            <th>Competition End-Date</th>
                        </tr>
                    </thead>
                    <tbody>
                        @foreach (var item in Model)
                        {
                            <tr>
                                <td>@item.CompetitionTitle</td>
                                <td> <img src="~/@item.CompetitionBanner" class="d-flex m-auto" width="100" height="100" alt="..."></td>
                                <td>@item.CompStartDate</td>
                                <td>@item.CompEndDate</td>
                            </tr>
                        }
                    </tbody>
                </table>
            </div>
        </div>
    </div>

</div>
